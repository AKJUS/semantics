<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="de.cau.cs.kieler.sim.kiem.json.datacomponent">
      <content
            class="de.cau.cs.kieler.s.sim.kivi.SVisualizationDataComponent"
            id="de.cau.cs.kieler.s.sim.kivi"
            name="S Visualization">
      </content>
   </extension>
   <extension
         point="de.cau.cs.kieler.sim.kiem.config.DefaultScheduleContributor">
      <executionFile
            file="execution/s_sc.execution">
         <editor
               id="de.cau.cs.kieler.s.S"
               name="KIELER S Editor"
               priority="10">
         </editor>
      </executionFile>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            category="de.cau.cs.kieler.sim"
            class="de.cau.cs.kieler.s.sim.kivi.view.SInstructionsView"
            icon="icons/SInstructionsViewIcon.png"
            id="de.cau.cs.kieler.s.sim.kivi.view.SInstructionView"
            name="S Instruction View"
            restorable="true">
      </view>
   </extension>
   <extension
         point="de.cau.cs.kieler.sim.kiem.json.datacomponent">
      <content
            class="de.cau.cs.kieler.s.sim.kivi.view.InstructionViewDataComponent"
            id="de.cau.cs.kieler.s.sim.kivi.view"
            name="S Instruction View">
      </content>
   </extension>

</plugin>
