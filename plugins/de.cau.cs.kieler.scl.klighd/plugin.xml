            class="de.cau.cs.kieler.klighd.transformations.GuiceBasedTransformationFactory:de.cau.cs.kieler.scl.klighd.scg.SCGDiagramSynthesis"
            id="de.cau.cs.kieler.scl.klighd.scg.SCGDiagramSynthesis">
      </transformation>
   </extension>
   <extension
         point="de.cau.cs.kieler.core.kivi.combinations">
