<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<plugin>

  <extension point="org.eclipse.emf.ecore.generated_package">
    <package 
       uri = "http://www.cau.de/cs/kieler/esterel/Esterel" 
       class = "de.cau.cs.kieler.esterel.esterel.EsterelPackage"
       genModel = "de/cau/cs/kieler/esterel/model/generated/Esterel.genmodel" />
    <package
          class="de.cau.cs.kieler.esterel.scest.scest.ScestPackage"
          genModel="de/cau/cs/kieler/esterel/model/generated/SCEst.genmodel"
          uri="http://www.cau.de/cs/kieler/esterel/scest/SCEst">
    </package> 
	
  </extension>
  <extension
        point="de.cau.cs.kieler.kico.extension">
     <ExtensionInformation
           ePackageName="esterel"
           editor_id="de.cau.cs.kieler.esterel.Esterel"
           extensionName="strl"
           isXMI="false">
     </ExtensionInformation>
     <ExtensionInformation
           ePackageName="scest"
           editor_id="de.cau.cs.kieler.esterel.scest.SCEst"
           extensionName="scest"
           isXMI="false">
     </ExtensionInformation>
  </extension>
  <extension
        point="de.cau.cs.kieler.kico.feature">
     <featureClass
           class="de.cau.cs.kieler.esterel.features.Esterel">
     </featureClass>
     <featureClass
           class="de.cau.cs.kieler.esterel.features.SimulationVisualization">
     </featureClass>
     <featureClass
           class="de.cau.cs.kieler.esterel.scest.features.EsterelParallel">
     </featureClass>
     <featureGroupClass
           class="de.cau.cs.kieler.esterel.scest.features.SCEstFeature">
     </featureGroupClass>
  </extension>
  <extension
        point="de.cau.cs.kieler.kico.transformation">
     <expansionTransformationClass
           class="de.cau.cs.kieler.esterel.scest.transformations.EsterelParallel">
     </expansionTransformationClass>
  </extension>
