<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="scest" nsURI="http://www.cau.de/cs/kieler/esterel/scest/SCEst"
    nsPrefix="scest">
  <eClassifiers xsi:type="ecore:EClass" name="SCEstProgram">
    <eStructuralFeatures xsi:type="ecore:EReference" name="modules" upperBound="-1"
        eType="#//SCEstModule" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="SCEstModule">
    <eStructuralFeatures xsi:type="ecore:EReference" name="annotations" upperBound="-1"
