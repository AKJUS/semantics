<plugin
      name="%pluginName"
      id="de.cau.cs.kieler.sccharts.edit"
      version="1.0.0"
      provider-name="%providerName"
      class="de.cau.cs.kieler.sccharts.provider.SCChartsEditPlugin$Implementation">

   <requires>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="de.cau.cs.kieler.sccharts" export="true"/>
      <import plugin="org.eclipse.emf.edit" export="true"/>
      <import plugin="de.cau.cs.kieler.annotations" export="true"/>
      <import plugin="de.cau.cs.kieler.annotations.edit" export="true"/>
      <import plugin="de.cau.cs.kieler.kexpressions" export="true"/>
      <import plugin="de.cau.cs.kieler.kexpressions.edit" export="true"/>
      <import plugin="de.cau.cs.kieler.scl" export="true"/>
      <import plugin="de.cau.cs.kieler.scl.edit" export="true"/>
   </requires>

   <runtime>
      <library name=".">
         <export name="*"/>
      </library>
   </runtime>

   <extension point="org.eclipse.emf.edit.itemProviderAdapterFactories">
      <!-- @generated sccharts -->
      <factory
            uri="http://kieler.cs.cau.de/sccharts/0.3.0"
            class="de.cau.cs.kieler.sccharts.provider.SCChartsItemProviderAdapterFactory"
            supportedTypes=
              "org.eclipse.emf.edit.provider.IEditingDomainItemProvider
               org.eclipse.emf.edit.provider.IStructuredItemContentProvider
               org.eclipse.emf.edit.provider.ITreeItemContentProvider
               org.eclipse.emf.edit.provider.IItemLabelProvider
               org.eclipse.emf.edit.provider.IItemPropertySource"/>
   </extension>

</plugin>
