knode not {
    properties: de.cau.cs.kieler.portConstraints = FIXED_ORDER
    size: width = 30 height = 25
    krectangle{
        
        kpolyline{
        styles: lineWidth = 0.5
            points: left 50.5%,bottom 15%; left 40%, bottom 2%; left 60%,bottom 2%; left 49.5%,bottom 15%   
        }
    }
  kport in0 {
    size: width = 0 height = 0
    properties: de.cau.cs.kieler.portSide = WEST
  }
 
  kport out {
    size: width = 0 height = 0
    pos:  x = 31
    properties: org.eclipse.elk.port.borderOffset = -0.5 
    de.cau.cs.kieler.portSide = EAST
  }
}