<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.commands">
      <category
            id="de.cau.cs.kieler.kico.web.commands.category"
            name="Sample Category">
      </category>
      <command
            categoryId="de.cau.cs.kieler.kico.web.commands.category"
            id="de.cau.cs.kieler.kico.web.commands.enableCommand"
            name="Enable KiCo Web Interface Command">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="de.cau.cs.kieler.kico.web.handlers.ButtonHandler"
            commandId="de.cau.cs.kieler.kico.web.commands.enableCommand">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
         <toolbar
               id="de.cau.cs.kieler.kico.web.toolbars.enableToolbar">
            <command
                  commandId="de.cau.cs.kieler.kico.web.commands.enableCommand"
                  icon="icons/KiCoViewIcon.png"
                  id="de.cau.cs.kieler.kico.web.toolbars.enableCommand"
                  tooltip="KIELER Compiler Web Interface">
            </command>
         </toolbar>
      </menuContribution>
   </extension>

</plugin>
