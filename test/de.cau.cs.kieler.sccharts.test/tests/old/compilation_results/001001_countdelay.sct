scchart countdelay {
  input bool I;
  output bool O;
  int _counter;

  initial state A {

    region _Entry:

    initial state _Init
    --> _Done immediate / _counter = 0;

    final state _Done;
  }
  >-> _Exit;

  state B
  --> A immediate / O = O | true;

  state _Exit
  --> _Depth;

  state _Depth
  --> B immediate with _counter == 5
  --> _Exit immediate;

  region _During:

  state _Depth
  --> _I immediate / O = false;

  initial state _I
  --> _Depth;

  region _During2:

  state _S
  --> _I immediate / _counter = 1 + _counter;

  initial state _I
  --> _Depth;

  state _Depth
  --> _S immediate with I
  --> _I immediate;
}