scchart ComplexFinalState {
  input bool I;

  initial state A {
    bool _abort;
    bool _term;

    region _Entry:

    state _Main {

      state A1
      --> _Final immediate with _abort
      --> _Pause2 immediate;

      final state _Final;

      state _Depth
      --> A1 immediate / _term = true;

      state _S2
      --> A0 immediate / _term = false;

      initial state A0
      --> _Depth;

      state _Pause2
      --> _Depth2;

      state _Depth2
      --> _S2 immediate with I
      --> _S5 immediate;

      state _S5
      --> _Final immediate with _abort
      --> _Pause2 immediate;

      region _Watch:

      initial state _Watch
      --> _S immediate with _term
      --> _Pause immediate;

      final state _Aborted;

      state _S
      --> _Aborted immediate / _abort = true;

      state _Pause
      --> _Depth;

      state _Depth
      --> _S immediate with _term
      --> _Pause immediate;
    }
    >-> _Done;

    initial state _Init
    --> _C immediate / _abort = false;

    final state _Done;

    state _C
    --> _Main immediate / _term = false;
  }
  >-> F;

  final state F;
}