
@diagram[KLayLayered] false
@VHLayout
scchart ExitAction {
  output bool Ae4;
  output bool A5;

  initial state S1
  --> S4;

  state S4 {

    region _Entry:

    initial state _Main {

      region R1:

      initial state S41
      --> S42;

      final state S42;

      region R2:

      initial state S43
      --> S44;

      final state S44;
    }
    >-> _S immediate;

    final state _Done;

    state _S
    --> _Done immediate with / Ae4 = true;
  }
  >-> _S;

  final state S5;

  state _S
  --> S5 immediate with / A5 = true;
}