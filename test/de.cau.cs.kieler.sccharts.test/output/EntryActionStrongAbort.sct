
@diagram[KLayLayered] false
@VHLayout
scchart EntryActionStrongAbort {
  output bool A1;
  output bool A2;
  input bool T1;
  input bool T2;

  initial state S1
  --> S2;

  state S2 {

    state S21
    --> S22 immediate with T2
    --> _Pause immediate;

    final state S22;

    initial state _Init
    --> S21 immediate with / A1 = true;

    state _S
    --> S22 immediate with / A2 = true;

    state _Pause
    --> _Depth;

    state _Depth
    --> S22 immediate with T2
    --> _S3 immediate;

    state _S3
    --> _S immediate with T1
    --> _Pause immediate;
  }
  >-> _C;

  state _C
  --> _Depth immediate with T2
  --> _Depth immediate;

  state _Depth
  --> _Depth;
}