
@diagram[KLayLayered] false
@layout[direction] DOWN
@diagram[Declarations] false
scchart ALDO "AbortConnector" {
  input bool I;
  input bool I2;
  output bool D;
  output bool O;

  region Main " ":

  state On {

    initial state Do
    --> _Aborted immediate with I
    --> _Pause immediate;

    state _C
    --> _Depth2 immediate with I2
    --> Do immediate;

    state _Depth2
    --> _Aborted immediate with I
    --> _Pause2 immediate;

    final state _Aborted;

    state _S3
    --> _C immediate with / D = true;

    state _Pause
    --> _Depth;

    state _Depth
    --> _Aborted immediate with I
    --> _S3 immediate;

    state _Pause2
    --> _Depth2;
  }
  >-> _S immediate;

  initial state _Init
  --> _C immediate with / D = false;

  state _C
  --> On immediate with / O = false;

  state _S
  --> _Depth immediate with / O = true;

  state _Depth
  --> _Depth;
}