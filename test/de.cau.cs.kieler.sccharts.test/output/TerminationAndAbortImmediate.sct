scchart Cabin {
  input bool I;
  output bool O1;
  output bool O2;
  output bool O3;

  state S1 {

    initial state S1
    --> S2 immediate with I
    --> _Pause immediate;

    final state S2;

    state _S3
    --> S2 immediate with / O1 = true;

    state _Pause
    --> _Depth;

    state _Depth
    --> S2 immediate with I
    --> _S3 immediate;
  }
  >-> _Depth3;

  state _Depth3
  --> _S immediate with I
  --> _S4 immediate;

  initial state _Init
  --> _C2 immediate with / O1 = false;

  state _C2
  --> _C3 immediate with / O2 = false;

  state _C3
  --> S1 immediate with / O3 = false;

  state _S
  --> _Depth immediate with / O2 = true;

  state _Depth
  --> _Depth;

  state _Depth2
  --> _Depth2;

  state _S4
  --> _Depth2 immediate with / O3 = true;
}