scchart _77_several_same_state_ids_empty_region_ids
"77-several-same-state-ids-empty-region-ids" {
  input bool I;
  output bool O;
  output bool O2;
  output bool O3;

  initial state ^state {
    bool _trig;

    initial state ^state {

      region _Entry:

      state _Main {

        state _Depth2
        --> _Aborted immediate with _trig
        --> _Pause2 immediate;

        final state _Aborted;

        state _S
        --> _Depth2 immediate with / O = O || true;

        initial state ^state
        --> _Depth;

        state _Depth
        --> _S immediate with I
        --> _S3 immediate;

        state _S3
        --> _Aborted immediate with _trig
        --> ^state immediate;

        state _Pause2
        --> _Depth2;

        region:

        state _Depth2
        --> _Aborted2 immediate with _trig
        --> _Pause2 immediate;

        final state _Aborted2;

        state _Depth
        --> _Depth2 immediate with / O2 = O2 || true;

        initial state ^state
        --> _Depth;

        state _Pause2
        --> _Depth2;

        region _Ctrl:

        initial state _Run
        --> _S;

        final state _Done;

        state _S
        --> _Done immediate with / _trig = true;
      }
      >-> _Done;

      initial state _Init
      --> _Main immediate with / _trig = false;

      final state _Done;
    }
    >-> _S immediate;

    state _S
    --> _Depth immediate with / O3 = O3 || true;

    state _Depth
    --> _Depth;
  };

  region _During:

  state _S
  --> _Depth;

  initial state _Depth
  --> _S3 immediate with / O = false;

  state _S3
  --> _S4 immediate with / O2 = false;

  state _S4
  --> _S immediate with / O3 = false;
}