
@diagram[KLayLayered] false
@VHLayout
scchart EntryAction {
  input bool T1;
  output bool Ae2a, Ae2b;

  initial state S1
  --> S2;

  state S2 {

    region _Entry:

    state _Main {

      region R1:

      initial state S41
      --> S42;

      final state S42;

      region R2:

      initial state S43
      --> S44;

      final state S44;
    }
    >-> _Done;

    initial state _Init
    --> _C immediate with / Ae2a = true;

    final state _Done;

    state _C
    --> _S immediate with T1
    --> _Main immediate;

    state _S
    --> _Main immediate with / Ae2b = true;
  }
  >-> _Depth;

  state _Depth
  --> _Depth;
}