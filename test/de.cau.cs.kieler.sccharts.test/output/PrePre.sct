
@diagram[KLayLayered] false
scchart PrePre {
  output int x;
  output int y;
  int _pre_x;
  int _reg_x;
  int _pre__pre_x;
  int _reg__pre_x;

  region _Entry:

  state _Main {

    initial state S
    --> _S;

    state _S
    --> _S2 immediate with / x = x + 1;

    state _S2
    --> S immediate with / y = _pre__pre_x;

    region _Pre:

    initial state _Init
    --> _Wait immediate with / _reg_x = x;

    state _Wait
    --> _S;

    state _S
    --> _Init immediate with / _pre_x = _reg_x;

    region _Pre:

    initial state _Init
    --> _Wait immediate with / _reg__pre_x = _pre_x;

    state _Wait
    --> _S;

    state _S
    --> _Init immediate with / _pre__pre_x = _reg__pre_x;
  }
  >-> _Done;

  initial state _Init
  --> _C immediate with / x = 0;

  final state _Done;

  state _C
  --> _C2 immediate with / y = 0;

  state _C2
  --> _C3 immediate with / _pre_x = 0;

  state _C3
  --> _C4 immediate with / _reg_x = 0;

  state _C4
  --> _C5 immediate with / _pre__pre_x = 0;

  state _C5
  --> _Main immediate with / _reg__pre_x = 0;
}