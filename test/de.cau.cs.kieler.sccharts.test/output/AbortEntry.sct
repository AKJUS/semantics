
@diagram[KLayLayered] false
@VHLayout
scchart AbortComplexity {
  input bool R;
  output int O;

  region Main " ":

  initial state S {

    region _Entry:

    initial state _Init
    --> _Done immediate with / O ++;

    final state _Done;
  }
  >-> _Exit;

  state _S
  --> S immediate with / O = false;

  state _Exit
  --> _Depth;

  state _Depth
  --> _S immediate with R
  --> _Exit immediate;
}