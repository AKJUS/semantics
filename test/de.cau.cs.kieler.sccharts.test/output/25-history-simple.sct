scchart _20_history "20-history" {
  input bool I;
  output bool O;
  bool _trig;

  initial state A {

    region _Entry:

    state _Main {

      state _Depth2
      --> _Aborted immediate with _trig
      --> _Pause2 immediate;

      final state _Aborted;

      state _Depth
      --> _Depth2 immediate with / O = O || true;

      initial state C
      --> _Depth;

      state _Pause2
      --> _Depth2;

      region _Ctrl:

      final state _Done;

      state _S
      --> _Done immediate with / _trig = true;

      initial state _Run
      --> _Depth;

      state _Depth
      --> _S immediate with I
      --> _Run immediate;
    }
    >-> _Done;

    initial state _Init
    --> _Main immediate with / _trig = false;

    final state _Done;
  }
  >-> E immediate;

  state E
  --> _Depth;

  state _Depth
  --> A immediate with I
  --> E immediate;

  region _During:

  initial state _I
  --> _S immediate with / O = false;

  state _S
  --> _I;
}