scchart ABRO2 "ABRO" {
  input bool A;
  input bool B;
  output bool O;

  initial state WaitAB {

    final state dA;

    state _S
    --> dA immediate with / B = B || true;

    initial state wA
    --> _Depth;

    state _Depth
    --> _S immediate with A
    --> wA immediate;

    region:

    final state dB;

    initial state wB
    --> _Depth;

    state _Depth
    --> dB immediate with B
    --> wB immediate;
  }
  >-> _S;

  state _S
  --> _Depth immediate with / O = O || true;

  state _Depth
  --> _Depth;

  region _During:

  initial state _I
  --> _S immediate with / O = false;

  state _S
  --> _I;
}