
@diagram[KLayLayered] false
@VHLayout
scchart EntryAction {
  input bool T1;
  output bool Ae2a, Ae2b;

  initial state S1
  --> S2;

  state S2 {

    region _Entry:

    initial state _Init
    --> _C immediate with / Ae2a = true;

    final state _Done;

    state _C
    --> _S immediate with T1
    --> _Done immediate;

    state _S
    --> _Done immediate with / Ae2b = true;
  }
  >-> _Exit;

  state _Exit
  --> _Depth;

  state _Depth
  --> _Depth;
}