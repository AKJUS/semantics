scchart _76ptolemyconcurrecntandhierarchicalreadwrite
"76ptolemyconcurrecntandhierarchicalreadwrite" {
  output bool OutA;
  bool In;
  output bool Out2;
  input bool Const;
  output bool OutB;

  initial state SeeminglyNonmontonic {
    bool _trig;

    initial state state1 {

      region _Entry:

      state _Main {

        state _Depth2
        --> _Aborted immediate with _trig
        --> _Pause2 immediate;

        final state _Aborted;

        state _S
        --> _Depth2 immediate with / OutA = OutA || true;

        initial state state5
        --> _Depth;

        state _Depth
        --> _S immediate with In
        --> _S3 immediate;

        state _S3
        --> _Aborted immediate with _trig
        --> state5 immediate;

        state _Pause2
        --> _Depth2;

        region:

        state _Depth2
        --> _Aborted2 immediate with _trig
        --> _Pause2 immediate;

        final state _Aborted2;

        state _Depth
        --> _Depth2 immediate with / OutB = OutB || true;

        initial state state3
        --> _Depth;

        state _Pause2
        --> _Depth2;

        region _Ctrl:

        initial state _Run
        --> _S;

        final state _Done;

        state _S
        --> _Done immediate with / _trig = true;
      }
      >-> _Done;

      initial state _Init
      --> _Main immediate with / _trig = false;

      final state _Done;
    }
    >-> _S immediate;

    state _S
    --> _Depth immediate with / Out2 = Out2 || true;

    state _Depth
    --> _Depth;
  };

  region:

  state _S
  --> Const immediate with / In = In || true;

  initial state Const
  --> _Depth;

  state _Depth
  --> _S immediate with Const
  --> Const immediate;

  region _During:

  state _S
  --> _Depth;

  initial state _Depth
  --> _S3 immediate with / OutA = false;

  state _S3
  --> _S4 immediate with / In = false;

  state _S4
  --> _S5 immediate with / Out2 = false;

  state _S5
  --> _S immediate with / OutB = false;
}