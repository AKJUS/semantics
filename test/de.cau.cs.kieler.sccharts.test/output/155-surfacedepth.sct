scchart _155_surfacedepth "155-surfacedepth" {
  input bool I1;
  input bool I2;
  output bool O1;
  output bool O2;

  initial state A
  --> _S immediate with I1
  --> _Pause immediate;

  state _S
  --> _Depth2 immediate with / O1 = O1 || true;

  state _S2
  --> _Depth3 immediate with / O2 = O2 || true;

  state _Pause
  --> _Depth;

  state _Depth
  --> _S immediate with I1
  --> _S4 immediate;

  state _S4
  --> _S2 immediate with I2
  --> _Pause immediate;

  state _Depth2
  --> _Depth2;

  state _Depth3
  --> _Depth3;

  region _During:

  state _S
  --> _Depth;

  initial state _Depth
  --> _S3 immediate with / O1 = false;

  state _S3
  --> _S immediate with / O2 = false;
}