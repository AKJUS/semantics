scchart _105_weakabort_hierarchy_dependency "105-weakabort-hierarchy-dependency"
{
  input bool I;
  output bool O;
  bool L;
  output bool O2;
  bool _trig;

  initial state A {
    bool _trig2;

    region _Entry:

    state _Main {

      initial state B {

        region _Entry:

        state _Main {

          state _Depth2
          --> _Aborted2 immediate with _trig2
          --> _Pause2 immediate;

          final state _Aborted2;

          state _S
          --> _S2 immediate with / L = L || true;

          state _S2
          --> _Depth2 immediate with / O = O || true;

          initial state C
          --> _Depth;

          state _Depth
          --> _S immediate with I
          --> _S4 immediate;

          state _S4
          --> _Aborted2 immediate with _trig2
          --> C immediate;

          state _Pause2
          --> _Depth2;

          region _Ctrl2:

          final state _Done2;

          state _S
          --> _Done2 immediate with / _trig2 = true;

          initial state _Run2
          --> _Depth;

          state _Depth
          --> _S immediate with _trig
          --> _Run2 immediate;
        }
        >-> _Done;

        initial state _Init
        --> _Main immediate with / _trig2 = false;

        final state _Done;
      }
      >-> _Aborted immediate;

      final state _Aborted;

      region _Ctrl:

      final state _Done;

      state _S
      --> _Done immediate with / _trig = true;

      initial state _Run
      --> _Depth;

      state _Depth
      --> _S immediate with L
      --> _Run immediate;
    }
    >-> _Done;

    initial state _Init
    --> _Main immediate with / _trig = false;

    final state _Done;
  }
  >-> _S immediate;

  state _S
  --> _Depth immediate with / O2 = O2 || true;

  state _Depth
  --> _Depth;

  region _During:

  state _S
  --> _Depth;

  initial state _Depth
  --> _S3 immediate with / O = false;

  state _S3
  --> _S4 immediate with / L = false;

  state _S4
  --> _S immediate with / O2 = false;
}