scchart Variables {
  output bool b;
  output int i;
  output float f;
  output bool s;
  output bool vs;
  output int vs_val combine +;
  int vs_curval combine +;

  region _Entry:

  state _Main {

    initial state S0
    --> _S;

    state S1
    --> _S4;

    state S2
    --> _S6;

    state S3
    --> _S9;

    state _S
    --> _S2 immediate with / b = false;

    state _S2
    --> _S3 immediate with / i = - 1;

    state _S3
    --> S1 immediate with / f = - 2.1234;

    state _S4
    --> _S5 immediate with / i = f;

    state _S5
    --> S2 immediate with / f = i;

    state _S6
    --> _S7 immediate with / s = s || true;

    state _S7
    --> _S8 immediate with / vs_curval = vs_curval + 12;

    state _S8
    --> S3 immediate with / vs = vs || true;

    state _S9
    --> _Depth immediate with / i = vs_val;

    state _Depth
    --> _Depth;

    region _During:

    state _S
    --> _Depth;

    initial state _Depth
    --> _S3 immediate with / s = false;

    state _S3
    --> _S immediate with / vs = false;

    region _During2:

    initial state _Depth
    --> _S2 immediate with vs
    --> _Pause immediate;

    state _S
    --> _Depth;

    state _S2
    --> _S immediate with / vs_val = vs_curval;

    state _Pause
    --> _Depth;

    region _During3:

    initial state _I
    --> _S immediate with / vs_curval = 0;

    state _S
    --> _I;
  }
  >-> _Done;

  initial state _Init
  --> _C immediate with / b = true;

  final state _Done;

  state _C
  --> _C2 immediate with / i = 1;

  state _C2
  --> _Main immediate with / f = 2.5;
}