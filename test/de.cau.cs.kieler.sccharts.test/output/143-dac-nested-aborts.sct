scchart NestedAborts {
  input bool Otrig;
  input bool Itrig;
  output bool O;
  output bool O2;
  bool _trig;

  initial state Outer {
    bool _trig2;
    bool _trig3;

    region _Entry:

    state _Main {

      initial state Inner1 {

        region _Entry:

        state _Main {

          final state _Aborted2;

          initial state S
          --> _Depth;

          state _Depth
          --> _Aborted2 immediate with _trig2 || _trig3
          --> S immediate;

          region _Ctrl2:

          final state _Done2;

          state _S
          --> _Done2 immediate with / _trig2 = true;

          state _S2
          --> _Done2 immediate with / _trig3 = true;

          initial state _Run2
          --> _Depth;

          state _Depth
          --> _S immediate with Itrig
          --> _S4 immediate;

          state _S4
          --> _S2 immediate with _trig
          --> _Run2 immediate;
        }
        >-> _Done;

        initial state _Init
        --> _C immediate with / _trig2 = false;

        final state _Done;

        state _C
        --> _Main immediate with / _trig3 = false;
      }
      >-> _C2;

      state _Depth
      --> _Aborted immediate with _trig
      --> _Pause immediate;

      final state _Aborted;

      state _C2
      --> _S immediate with _trig2
      --> _Aborted immediate;

      state _S
      --> _Depth immediate with / O = O || true;

      state _Pause
      --> _Depth;

      region _Ctrl:

      final state _Done;

      state _S
      --> _Done immediate with / _trig = true;

      initial state _Run
      --> _Depth;

      state _Depth
      --> _S immediate with Otrig
      --> _Run immediate;
    }
    >-> _Done;

    initial state _Init
    --> _Main immediate with / _trig = false;

    final state _Done;
  }
  >-> _S immediate;

  state _S
  --> _Depth immediate with / O2 = O2 || true;

  state _Depth
  --> _Depth;

  region _During:

  state _S
  --> _Depth;

  initial state _Depth
  --> _S3 immediate with / O = false;

  state _S3
  --> _S immediate with / O2 = false;
}