<?xml version="1.0" encoding="UTF-8"?>
<synccharts:Region xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:kexpressions="http://kieler.cs.cau.de/kexpressions/0.1.2" xmlns:synccharts="http://kieler.cs.cau.de/synccharts/0.2.4">
  <states id="CountDelays" label="CountDelays">
    <signals name="I" isInput="true"/>
    <signals name="O" isOutput="true"/>
    <variables name="V" type="int" initialValue="0" hostType=""/>
    <regions id="">
      <states id="S1" label="S1" isInitial="true" incomingTransitions="//@states.0/@regions.0/@states.0/@outgoingTransitions.0">
        <outgoingTransitions label="I / 'V++'" priority="1" targetState="//@states.0/@regions.0/@states.0">
          <effects xsi:type="synccharts:TextEffect" code="V++"/>
          <trigger xsi:type="kexpressions:ValuedObjectReference" valuedObject="//@states.0/@signals.0"/>
        </outgoingTransitions>
        <outgoingTransitions isImmediate="true" label="# 'V == 3' / O" priority="2" targetState="//@states.0/@regions.0/@states.1">
          <effects xsi:type="synccharts:Emission" signal="//@states.0/@signals.1"/>
          <trigger xsi:type="kexpressions:TextExpression" code="V == 3"/>
        </outgoingTransitions>
      </states>
      <states id="S2" label="S2" incomingTransitions="//@states.0/@regions.0/@states.0/@outgoingTransitions.1"/>
    </regions>
  </states>
</synccharts:Region>
