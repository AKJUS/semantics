          <states id="G" label="G" isInitial="true" incomingTransitions="//@states.0/@regions.0/@states.1/@regions.1/@states.1/@outgoingTransitions.0 //@states.0/@regions.0/@states.1/@regions.1/@states.1/@outgoingTransitions.1">
            <outgoingTransitions priority="1" targetState="//@states.0/@regions.0/@states.1/@regions.1/@states.1"/>
