            <outgoingTransitions isImmediate="true" label="# A / O" priority="1" targetState="//@states.0/@regions.0/@states.1/@regions.0/@states.2">
              <effects xsi:type="synccharts:Emission" signal="//@states.0/@signals.2"/>
